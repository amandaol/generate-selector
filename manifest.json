{
  "manifest_version": 2,

  "name": "Generate the Optimum jQuery Selector",
  "short_name": "jQuery Selector Generator",
  "description": "This extension generates the optimum jquery selector on alt+click of the desired element.",
  "version": "1.1",
  "author": "Amanda Olsen",

  "minimum_chrome_version": "10.0",

  "icons": {
  		"16": "icon16.png",
  		"48": "icon48.png",
        "128": "icon128.png" 
    },

   "web_accessible_resources": [
    "lock.png"
  ],

  "content_scripts" : [{
      "matches": ["http://*/*", "https://*/*"],
      "js" : ["jquery-1.11.3.min.js", "main.js"]
  }]
}
